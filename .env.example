# Tianting-v2 Environment Variables Template
# Copy this file to .env and fill in your actual values

# OpenAI API Configuration
OPENAI_API_KEY=your_api_key_here
OPENAI_BASE_URL=https://api.siliconflow.cn/v1

# Default Model Configuration  
DEFAULT_MODEL=Pro/deepseek-ai/DeepSeek-R1
# Alternative: Qwen/Qwen2.5-72B-Instruct

# Development Settings
NODE_ENV=development
DEBUG=false

# Tianting Configuration
WORKSPACE_ROOT=/home/${USER}/koko/tianting
CLAUDE_CMD=claude
MAX_PARALLEL=3
TMUX_SESSION=tianting